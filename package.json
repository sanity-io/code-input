    "@sanity/incompatible-plugin": "^0.0.1-studio-v3.1",
    "@sanity/ui": "^0.37.8",
    "ace-builds": "^1.9.5",
    "react-ace": "^10.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^16.1.0",
