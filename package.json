    "@parcel/packager-ts": "^2.6.0",
    "@parcel/transformer-typescript-types": "^2.6.0",
    "@sanity/plugin-kit": "^0.0.1-studio-v3.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-sanity": "^5.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^7.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^12.4.1",
    "parcel": "^2.6.0",
    "prettier": "^2.6.2",
    "react": "^17.0.0 || ^18.0.0",
    "rimraf": "^3.0.2",
    "sanity": "^3.0.0-dev-preview.13",
    "semantic-release": "^19.0.3",
    "styled-components": "^5.2.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "sanity": "dev-preview",
    "styled-components": "^5.2.0"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/code-input/issues"
  },
  "homepage": "https://www.sanity.io/",
  "sanityExchangeUrl": "https://www.sanity.io/plugins/code-input",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/code-input.git"
  },
