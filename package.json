    "styled-components": "^5.3.6",
    "ts-jest": "^28.0.7",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "sanity": "^3",
    "styled-components": "^5.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sanityExchangeUrl": "https://www.sanity.io/plugins/code-input",
  "sanityPlugin": {
    "verifyPackage": {
      "babelConfig": false
    }
  }
